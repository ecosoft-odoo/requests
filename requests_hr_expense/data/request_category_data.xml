<odoo noupdate="1">
    <record id="request_category_data_hr_expense" model="request.category">
        <field name="name">Employee Expense</field>
        <field
            name="image"
            type="base64"
            file="requests_hr_expense/static/src/img/expense.png"
        />
        <field
            name="approved_action_id"
            ref="action_request_expense_approved_actions"
        />
        <field name="sequence">100</field>
        <field name="use_ex" eval="True" />
        <field name="automated_sequence" eval="True" />
        <field name="sequence_code">RQEX</field>
        <field name="has_date">optional</field>
        <field name="has_product">optional</field>
        <field name="has_quantity">optional</field>
    </record>
</odoo>
